#include "platform.h"
#include "system.h"
#include "xparameters.h"

#include "nexys4io.h"

int main(void)
{
	uint32_t status;

	/* Initialize the system */
	init_platform();
	status = do_init();

	/* Enable global interrupts */
	microblaze_enable_interrupts();

	/* Init OK? */
	if (status != XST_SUCCESS)
		while (1);
}
